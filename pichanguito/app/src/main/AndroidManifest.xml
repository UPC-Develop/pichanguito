<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.upc.pichanguito">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Pichanguito">
        <activity
            android:name=".ListarProductActivity"
            android:label="Productos"
            android:exported="false" />
        <activity
            android:name=".GuardarActivity"
            android:label="Guardar Nuevo Producto"
            android:exported="false" />
        <activity
            android:name=".ResetUserPasswordActivity"
            android:exported="false" />
        <activity
            android:name=".HomeActivity"
            android:exported="false" />
        <activity
            android:name=".RegisterUserActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:label="Registrar Producto"
            android:parentActivityName=".ListarProductActivity"
            android:exported="true">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>